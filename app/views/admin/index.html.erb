<h2>Users</h2>
<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
    <tr>
      <th>Email</th>
    </tr>
    </thead>
    <tbody>

    <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
        </tr>
    <% end %>

    </tbody>
  </table>
</div>